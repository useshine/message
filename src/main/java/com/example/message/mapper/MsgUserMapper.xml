<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.message.mapper.MsgUserMapper">
    <insert id="Add" parameterType="com.example.message.entity.MsgUser" useGeneratedKeys="true" keyProperty="id">
        insert into msg_user(msgId, userId, createTime)
        values (#{msgId}, #{userId}, #{createTime})
    </insert>

</mapper>